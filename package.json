{
  "name": "cypress-multi-framework-testing",
  "version": "1.0.0",
  "description": "Multi-framework test suite for the Cypress Example Kitchen Sink app",
  "main": "index.js",
  "scripts": {
    "test:playwright": "playwright test",
    "test:selenium": "mocha test_scripts/selenium/**/*.test.js",
    "test:puppeteer": "mocha test_scripts/puppeteer/**/*.test.js",
    "test": "npm run test:playwright && npm run test:selenium && npm run test:puppeteer",
    "report": "node scripts/generate-report.js"
  },
  "keywords": [
    "testing",
    "playwright",
    "selenium",
    "puppeteer",
    "cypress",
    "automation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@playwright/test": "^1.42.1",
    "axios": "^1.6.7",
    "chai": "^4.3.10",
    "html-pdf": "^3.0.1",
    "mocha": "^10.2.0",
    "puppeteer": "^22.2.0",
    "selenium-webdriver": "^4.17.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.8.5",
    "lighthouse": "^11.6.0",
    "mocha-junit-reporter": "^2.2.1",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0"
  }
}